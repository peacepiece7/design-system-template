{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "storybook-static/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["//#dev:setup"]
    },
    "//#dev:setup": {
      "outputs": [".codegen/**"]
    },
    "dev:web": {
      "cache": false,
      "persistent": true
    },
    "dev:docs": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "dependsOn": ["@repo/ui#test", "@app/storybook#build"]
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    }
  }
}
